{
    "name": "henri-potier-react",
    "version": "0.1.0",
    "description": "",
    "author": "Vincent COUTY",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "webpack-dev-server --mode development",
        "build": "webpack --mode production",
        "test": "jest --config ./jest.config.json",
        "test:watch": "npm run test -- --watch",
        "pree2e": "rimraf e2e/screenshots",
        "e2e": "testcafe",
        "pree2e:json": "rimraf e2e/reports && rimraf e2e/screenshots",
        "e2e:json": "testcafe chrome --reporter cucumber-json:e2e/reports/report.json --reporter-app-name='Henri Potier React' --reporter-app-version='x.y.z'",
        "e2e:live": "yarn e2e --live",
        "e2e:report": "yarn e2e:json ; babel-node e2e/e2e-report-generator.js",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix && stylelint '*/**/*.scss'"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.1.2"
    },
    "devDependencies": {
        "@babel/node": "^7.8.7",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.4",
        "@types/jest": "^25.2.1",
        "@types/react": "^16.9.29",
        "@types/react-dom": "^16.9.5",
        "@types/react-router-dom": "^5.1.3",
        "@types/vfile-message": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "babel-jest": "^25.2.4",
        "babel-loader": "^8.1.0",
        "css-loader": "^3.4.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "html-webpack-plugin": "^4.0.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^25.2.4",
        "mini-css-extract-plugin": "^0.9.0",
        "multiple-cucumber-html-reporter": "^1.16.1",
        "node-sass": "^4.13.1",
        "prettier": "^2.0.3",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3",
        "stylelint": "^13.3.0",
        "stylelint-config-standard": "^20.0.0",
        "testcafe": "^1.8.4",
        "testcafe-reporter-cucumber-json": "^5.0.0",
        "ts-jest": "^25.3.1",
        "ts-loader": "^6.2.2",
        "typescript": "^3.8.3",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    }
}
